#ifndef PARQUEO_H
#define PARQUEO_H
#include <string>

using namespace std;

class Parqueo{
	private:
		int n;
		int m;
		int z;
		double altura;
	public:
		Parqueo();
		Parqueo(int, int,double);

		void setN(int);
		int getN();
		
		void setM(int);
		int getM();

		void setAltura(double);
		double getAltura();

		~Parqueo();

};

#endif
